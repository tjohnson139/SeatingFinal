<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title WCCCA Seating Chart</title>
    <link rel="icon" href="~/Content/Images/wccca.png"
          type="image/x-icon">
    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="plugins/fontawesome-free/css/all.min.css">
    <!-- IonIcons -->
    <link rel="stylesheet" href="http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <script src="~/Scripts/Site.js" type="text/javascript"></script>

</head>
<body>

    <!-- Navbar -->
    <nav class="navbar navbar-expand navbar-light navbar-white" style="margin-left:-43em;">
        <div class="container">
            <div class="navbar-brand">
                <img src="~/Content/Images/wccca.png" alt="WCCCA Logo" class="brand-image elevation-3"
                     style="opacity: .8; height:75px; width:125px; margin-right:1em">
                <span class="font-weight-light ">WCCCA Seating Chart</span>
            </div>

            <!-- Right navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                </li>
                <li class="nav-item" style="margin-left:-45.5em">
                    <a href="../Account/Login" class="nav-link">Lead Login</a>
                </li>
            </ul>
        </div>
    </nav>
    <!-- /.navbar -->
</body>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
}
</html>